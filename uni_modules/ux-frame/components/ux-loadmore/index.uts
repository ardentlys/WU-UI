
export const UxLoadMoreMixin = defineMixin({
	emits: ['click'],
	data() {
		return {
			
		}
	},
	props: {
		state: {
			type: Number,
			default: 0,
		},
		texts: {
			type: Array,
			default: (): string[] => {
				return ['已加载', '加载中...', '-- 我也是有底线的 --']
			},
		}
	},
	methods: {
		click(e: MouseEvent) {
			this.$emit('click', e)
		}
	}
})